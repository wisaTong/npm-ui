{
  "name": "npm-ui-plus",
  "version": "0.2.1",
  "description": "gui project manager for npm project",
  "main": "dist/server.js",
  "bin": "dist/server.js",
  "scripts": {
    "start": "npm run build && node dist/server.js",
    "dev:front": "webpack-dev-server --mode development",
    "dev:back": "nodemon server.js --exec babel-node",
    "prebuild": "rm -rf dist && cp -r public/ dist/",
    "build": "webpack && babel . -D -d dist --ignore \"./node_modules/\" --ignore \"./src/client/\" --ignore \"./dist/\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wisaTong/npm-ui.git"
  },
  "author": "wisatong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wisaTong/npm-ui/issues"
  },
  "homepage": "https://github.com/wisaTong/npm-ui#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "nedb": "^1.8.0",
    "nedb-promises": "^4.0.0",
    "nodemon": "^2.0.2",
    "preact": "^10.1.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.3.0",
    "run-script-os": "^1.0.7",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "keywords": []
}